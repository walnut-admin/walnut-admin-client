{
  "name": "walnut-admin-client",
  "type": "module",
  "version": "1.4.0",
  "description": "An admin template, using `vue3`,`ts`,`vite`,`naive-ui`. Continuously updating...",
  "author": {
    "name": "Zhaocl1997",
    "email": "zhaocl97@foxmail.com",
    "url": "https://github.com/Zhaocl1997"
  },
  "license": "MIT",
  "homepage": "https://github.com/walnut-admin",
  "repository": {
    "type": "git",
    "url": "https://github.com/walnut-admin/walnut-admin-client.git"
  },
  "bugs": {
    "url": "https://github.com/walnut-admin/walnut-admin-client/issues"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "urls": {
    "doc": "https://walnut-admin-doc.netlify.app",
    "demo": "https://www.walnut-admin.com"
  },
  "scripts": {
    "predev": "esno build/generate/index.ts && npm run icon:dev",
    "dev": "vite",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx simple-git-hooks",
    "prebuild": "esno build/generate/index.ts && npm run icon:prod",
    "build": "vite build",
    "build:stage": "npm run prebuild && vite build --mode staging",
    "preview": "vite preview --mode production",
    "preview:stage": "vite preview --mode staging",
    "release": "esno release.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix --concurrency=auto",
    "commit:msg": "commitlint --edit",
    "types:check": "vue-tsc --noEmit",
    "types:check:log": "esno build/types.ts",
    "clean:cache": "rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "check:deps:update": "npx taze major -l",
    "check:nm": "node-modules-inspector",
    "deploy:stage": "deploy-cli-service deploy --mode staging",
    "deploy:prod": "deploy-cli-service deploy --mode production",
    "icon:dev": "esno build/icon dev",
    "icon:prod": "esno build/icon prod",
    "generate-pwa-assets": "pwa-assets-generator"
  },
