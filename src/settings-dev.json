{
  "themes": {
    "light": {
      "primaryColor": "#18a058",
      "infoColor": "#2080f0",
      "successColor": "#18a058",
      "warningColor": "#f0a020",
      "errorColor": "#d03050",
      "bodyColor": "#f8f8f8",
      "invertedColor": "#2C3E50"
    },
    "dark": {
      "primaryColor": "#63e2b7",
      "infoColor": "#70c0e8",
      "successColor": "#63e2b7",
      "warningColor": "#f2c97d",
      "errorColor": "#e88080",
      "bodyColor": "#202020",
      "invertedColor": "#2C3E50"
    }
  },
  "app": {
    "keepAlive": true,
    "scrollMode": "wrapper",
    "contentPadding": 4
  },
  "logo": {
    "id": "walnut-admin-logo",
    "status": true,
    "fixed": true,
    "transition": "slide-left"
  },
  "header": {
    "id": "walnut-admin-header",
    "status": true,
    "fixed": true,
    "transition": "slide-up",
    "height": 48,
    "scrollUpShow": false,
    "liveOnHover": false
  },
  "tabs": {
    "id": "walnut-admin-tabs",
    "status": true,
    "fixed": true,
    "height": 36,
    "itemWidth": 120,
    "transition": "slide-up",
    "persistent": true,
    "itemTransition": "fade-down",
    "scrollUpShow": false,
    "liveOnHover": false,
    "showUtils": true,
    "utilsMode": "overflow",
    "contextMenu": true,
    "sortable": true
  },
  "breadcrumb": {
    "id": "walnut-admin-breadcrumb",
    "status": true,
    "transition": "fade-down",
    "separator": ">"
  },
  "menu": {
    "id": "walnut-admin-menu",
    "status": true,
    "transition": "slide-left",
    "indent": 16,
    "width": 240,
    "iconSize": 24,
    "collapsedIconSize": 20,
    "collapsedWidth": 64,
    "collapseStatus": true,
    "accordion": true
  },
  "footer": {
    "id": "walnut-admin-footer",
    "status": true,
    "fixed": true,
    "height": 28,
    "transition": "slide-down",
    "content": "Copyright Â© 2020-present Walnut Admin. All Rights Reserved."
  }
}
